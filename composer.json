{
  "name": "passionweb/bayernportal-api",
  "description": "Shows the integration of the BayernPortal API. (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "typo3/cms-core": "^11.5 | ^12.4"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\BayernportalApi\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/bayernportal_api": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "bayernportal_api"
    }
  }
}
